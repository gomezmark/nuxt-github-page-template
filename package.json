{
  "name": "nuxt-github-page-template",
  "version": "1.0.0",
  "description": "My dazzling Nuxt.js project",
  "author": "gomezmark",
  "homepage": "https://gomezmark.github.io/nuxt-github-page-template/",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
