  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.0.0",
    "cross-env": "^6.0.3",
    "gh-pages": "^2.2.0",
    "push-dir": "^0.4.1"
  }
}
